@model WebApp.ViewModels.Test.TestViewModel

@{
    ViewData["Title"] = "Test";
}


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Instructions.FirstOrDefault()!.Description)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Instructions.FirstOrDefault()!.Name)
            </th>



        </tr>
    </thead>
    <tbody>
@foreach (var item in Model!.Instructions) {
        <tr>
            <td>
                @item.Description
            </td>

            <td>
                @item.Name
            </td>


        </tr>
}
    </tbody>
</table>
